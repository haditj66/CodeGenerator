        <Button x:Name="Button2" Content="Button2" HorizontalAlignment="Left" Margin="362,112,0,0" VerticalAlignment="Top" Width="75" Click="Button2_Click"/>
        <Button x:Name="Button3" Content="Button3" HorizontalAlignment="Left" Margin="362,162,0,0" VerticalAlignment="Top" Width="75" Click="Button3_Click"/>
        <Button x:Name="Button4" Content="Button4" HorizontalAlignment="Left" Margin="362,208,0,0" VerticalAlignment="Top" Width="75" Click="Button4_Click"/>
        <Button x:Name="Button5" Content="Button5" HorizontalAlignment="Left" Margin="362,244,0,0" VerticalAlignment="Top" Width="75" Click="Button5_Click"/>

    </Grid>
</Window>
